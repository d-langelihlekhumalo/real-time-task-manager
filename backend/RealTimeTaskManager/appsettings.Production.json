{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning",
			"Microsoft.EntityFrameworkCore": "Warning"
		},
		"Console": {
			"LogLevel": {
				"Default": "Information"
			}
		}
	},
	"AllowedHosts": "*",
	"ConnectionStrings": {
		"DefaultConnection": ""
	},
	"Cors": {
		"AllowedOrigins": ["https://yourdomain.com", "https://www.yourdomain.com"],
		"AllowCredentials": true
	},
	"HealthChecks": {
		"Enabled": true,
		"DetailedErrors": false
	},
	"SwaggerUI": {
		"Enabled": true
	}
}
